$('#new-iconup').hide();

var $iconupMarker = $("<%=  j(render(@iconup)) %>");
var $iconupDetails = $("<%= j(render partial: 'iconups/show', locals: {iconup: @iconup}) %>");

$('#floorplan').append($iconupMarker);
$('#floorplan').append($iconupDetails);


$iconupMarker.click(function(){
	$iconupDetails.show();
});

$iconupDetails.click(function() {
	$(this).hide();
})

